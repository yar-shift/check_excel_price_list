function onOpen() {
  var ui = SpreadsheetApp.getUi();
  ui.createMenu("‚öôÔ∏è –ú–µ–Ω—é")
    .addItem("üìù –í–Ω–µ—Å—Ç–∏ –¥–∞–Ω–Ω—ã–µ –≤ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏", "showForm")  // –µ—Å–ª–∏ —ç—Ç–æ—Ç –ø—É–Ω–∫—Ç –ø–æ–¥—Ä–∞–∑—É–º–µ–≤–∞–µ—Ç –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö –≤ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
    .addItem("üîÆ –û–±—Ä–∞–±–æ—Ç–∞—Ç—å –ø—Ä–∞–π—Å—ã", "importData")
    .addSeparator()
    .addItem("üåà –†–∞—Å–∫—Ä–∞—Å–∏—Ç—å –¥–∞–Ω–Ω—ã–µ", "applyConditionalFormatting")  // —É—Ç–æ—á–Ω–∏–ª –æ–ø–∏—Å–∞–Ω–∏–µ
    .addItem("üóÇÔ∏è –°—Ñ–æ—Ä–º–∏—Ä–æ–≤–∞—Ç—å –∑–∞–∫–∞–∑—ã –ø–æ –ª—É—á—à–∏–º —Ü–µ–Ω–∞–º", "createOrdersForEachSupplier")  // –æ–±–Ω–æ–≤–∏–ª –æ–ø–∏—Å–∞–Ω–∏–µ
    .addItem("üì¨ –û—Ç–ø—Ä–∞–≤–∏—Ç—å –≤ –¢–µ–ª–µ–≥—É", "sendToTelegram")  // –ø—Ä–µ–¥–ø–æ–ª–∞–≥–∞—é, —á—Ç–æ –∑–¥–µ—Å—å –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –¥—Ä—É–≥–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏
    .addToUi();
}
